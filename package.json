{
  "name": "booking-service-api",
  "version": "1.0.0",
  "keywords": [
    "koa2"
  ],
  "scripts": {
    "start": "node app.js && npm run migrate",
    "start_lily": "APP_ENV=lily node app.js",
    "start_local": "APP_ENV=local node app.js",
    "start_aws": "APP_ENV=aws node app.js",
    "start_ali_dev": "APP_ENV=ali-dev node app.js",
    "start_ali_prod": "APP_ENV=ali-prod node app.js",
    "start_aws_prod": "APP_ENV=aws-prod node app.js",
    "start_prod": "APP_ENV=prod node app.js",
    "start_likura": "APP_ENV=likura node app.js",
    "start_jake": "APP_ENV=jake node app.js",
    "start_bill": "cross-env APP_ENV=bill node app.js",
    "start_junius": "APP_ENV=junius node app.js",
    "migrate": "migrate up --store=./src/state/custom-state-storage",
    "doc": "apidoc -i src/api/ -o docs/apidoc/"
  },
  "dependencies": {
    "@kafkajs/confluent-schema-registry": "^2.0.1",
    "axios": "^0.21.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "faker": "^4.1.0",
    "form-data": "^3.0.0",
    "js-base64": "^2.5.1",
    "jsdom": "^16.4.0",
    "json5": "^0.4.0",
    "jsonwebtoken": "^8.3.0",
    "kafkajs": "^1.15.0",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "^5.2.0",
    "koa-router": "7.4.0",
    "koa-sslify": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-multiparty": "^1.0.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "puppeteer": "2.1.1",
    "q": "^1.4.0",
    "rate-limiter-flexible": "^2.1.9",
    "redis": "^3.0.2",
    "request": "2.72.0",
    "request-promise": "^4.2.6",
